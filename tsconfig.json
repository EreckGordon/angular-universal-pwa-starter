{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": "./",
    "paths": {
      "@environments/*": ["src/environments/*"],
      "@interfaces/*": ["src/shared/interfaces/*"],
      "@seo/*": ["src/client/app/shared/seo/*"],
      "@ngsw/*": ["src/client/app/shared/ngsw/*"]

    }
  }
}
