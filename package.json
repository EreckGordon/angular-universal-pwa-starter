  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "localWebpackForServer": "./node_modules/.bin/webpack",
    "prep": "ng build --prod --build-optimizer && ng build --prod --app 1 --output-hashing none --build-optimizer && npm run localWebpackForServer",
    "copy-frontend-to-deploy-folder": "robocopy \"C:\\Users\\Ereck\\Desktop\\angular-universal-pwa-starter-project\\angular-universal-pwa-starter\\dist\" \"C:\\Users\\Ereck\\Desktop\\angular-universal-pwa-starter-project\\angular-universal-pwa-starter-deploy\\dist\" /mir /xd .git /xf .gitattributes /xf .gitignore || ECHO.",
    "push-dist-to-github": "cd .. && cd angular-universal-pwa-starter-deploy && git add . && git commit -m \"auto generated commit\" && git push origin master",
    "deploy": "npm run prep && npm run copy-frontend-to-deploy-folder && npm run push-dist-to-github",
    "serverLocal": "npm run prep && npm run copy-frontend-to-deploy-folder && cd .. && cd angular-universal-pwa-starter-deploy && node server.js",
    "analyze-deploy": "ng build --prod --stats-json --build-optimizer && webpack-bundle-analyzer dist/stats.json",
    "analyze": "ng build --aot --stats-json && webpack-bundle-analyzer dist/stats.json",
    "serverRebuild": "npm run localWebpackForServer && cd .. && cd angular-universal-pwa-starter-deploy && node server.js"
  },
