{
  "name": "angular-universal-pwa-starter",
  "version": "2.5.2",
  "author": "Ereck Gordon",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e",
    "prep-front-end": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "prep-server": "./node_modules/.bin/tsc -p nestjs.tsconfig.json",
    "copy-needed-files-to-deploy-folder": "node copy-to-deploy-folder.js",
    "deploy-to-github": "node deploy-to-github.js",
    "deploy": "npm run format && npm run prep-front-end && npm run prep-server && npm run copy-needed-files-to-deploy-folder && npm run deploy-to-github",
    "dev-server": "node server/index.js",
    "prep-dev-server": "npm run format && npm run prep-front-end && npm run dev-server",
    "analyze-deploy": "ng build --prod --stats-json --build-optimizer && npm run analyze-bundle",
    "analyze-aot": "ng build --aot --stats-json && npm run analyze-bundle",
    "format": "tsfmt -r",
    "analyze-bundle": "./node_modules/.bin/webpack-bundle-analyzer dist/dist-browser/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/platform-server": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@angular/service-worker": "^5.0.0",
    "@nestjs/common": "^4.4.1",
    "@nestjs/core": "^4.4.1",
    "@nestjs/microservices": "^4.4.1",
    "@nestjs/testing": "^4.4.1",
    "@nestjs/websockets": "^4.4.1",
    "@nguniversal/common": "^5.0.0-beta.5",
    "@nguniversal/express-engine": "^5.0.0-beta.5",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "argon2": "^0.16.2",
    "body-parser": "^1.17.2",
    "compression": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.1",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "password-validator": "^4.0.0",
    "pg": "^7.3.0",
    "pg-native": "^2.2.0",
    "pg-query-stream": "^1.1.1",
    "pg.js": "^4.1.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.2",
    "tslib": "^1.7.1",
    "typeorm": "^0.2.0-alpha.10",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/body-parser": "^1.16.5",
    "@types/compression": "^0.0.34",
    "@types/cookie-parser": "^1.4.1",
    "@types/core-js": "^0.9.43",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.37",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "~6.0.60",
    "awesome-typescript-loader": "^3.2.3",
    "codelyzer": "~3.1.1",
    "fs-extra": "^4.0.2",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-helpers": "^1.1.2",
    "ts-node": "^3.3.0",
    "tslint": "~5.3.2",
    "typescript": "~2.5.0",
    "typescript-formatter": "^7.0.0",
    "webpack-bundle-analyzer": "^2.9.2"
  }
}
